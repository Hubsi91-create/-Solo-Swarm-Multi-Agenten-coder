# Solo-Swarm Multi-Agent System - Environment Variables

# Anthropic API Key (required for production)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=

# Worker Configuration
WORKER_QUEUES=high_priority,default,low_priority
LOG_LEVEL=INFO

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000

# Agent Pool Configuration
MAX_AGENT_SLOTS=100

# Development Settings
ENVIRONMENT=development
DEBUG=False
